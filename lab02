1. SELECT ename, dname FROM emp INNER JOIN dept ON emp.deptno = dept.deptno ORDER BY ename;
2. SELECT empno, ename, mgr FROM emp;
3. SELECT ename, 12 * (sal+250) FROM emp;
4. SELECT ename FROM emp ORDER BY hiredate DESC;
5. SELECT ename, job, sal, deptno FROM emp WHERE job = 'CLERK';
6. SELECT * FROM emp WHERE comm > sal;
7. SELECT * FROM emp WHERE sal BETWEEN 1000 AND 2000;
8. SELECT * FROM emp WHERE ename LIKE 'S%';
9. SELECT * FROM emp WHERE ename LIKE '____';
10.SELECT * FROM emp WHERE mgr IS NULL;
11.SELECT * FROM emp WHERE job LIKE 'CLERK' AND sal>=1000 AND sal<2000;
12.SELECT * FROM emp WHERE deptno BETWEEN 10 AND 20 ORDER BY ename;

